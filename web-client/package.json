{
  "name": "fury_dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "serve dist -p 3001",
    "test": "jest",
    "build": "webpack --mode production",
    "webpack-dev": "webpack serve -o ../dist/",
    "build-to-server": "webpack --mode production -o ../static/",
    "dev": "concurrently \"webpack --watch\" \"serve dist -p 3001\"",
    "dev-local-no-backend": "webpack serve --env from_yaml=true --env offline=true",
    "dev-local-with-backend": "concurrently \"webpack serve --env from_yaml=true --env offline=true\" \"go run ../main.go\""
  },
  "dependencies": {
    "@vaadin/router": "^1.7.4",
